public class quadrado2 {
	    public static void main(String[] args) {

	        int A = 7;
	        int B = -10;
	        int C = 13;

	        int soma = A + B + C;
	        int quadradoDaSoma = soma * soma;

	        System.out.println("O quadrado da soma de A, B e C Ã©: " + quadradoDaSoma);
	    }
	}
